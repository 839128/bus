        </dependency>
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>bus-sensitive</artifactId>
            <version>${project.version}</version>
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>${project.groupId}</groupId>
